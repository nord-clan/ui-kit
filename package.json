{
  "name": "@nord-clan/ui-kit",
  "description": "Пакет базовых стилизованных компонентов MUI",
  "version": "1.0.9",
  "main": "./dist/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./internal": {
      "import": "./dist/esm/internal.js",
      "require": "./dist/internal.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "author": {
    "name": "Ivan Kornilov",
    "email": "ivan.kornilov@nordclan.com",
    "url": "https://t.me/xsolncu"
  },
  "license": "MIT",
  "keywords": [
    "material",
    "design",
    "ui",
    "react",
    "mui"
  ],
  "engines": {
    "node": ">=16.19.0",
    "npm": "please use yarn or pnpm",
    "yarn": ">=1",
    "pnpm": ">=6"
  },
  "scripts": {
    "build": "npm run build:rollup",
    "build:rollup": "rollup --config",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build --docs",
    "storybook:preview": "serve storybook-static",
    "test": "jest -i --coverage=false",
    "test:watch": "jest --watch",
    "test:coverage": "jest -i --coverage",
    "lint": "npm run lint:ts",
    "lint:ts": "eslint \"src/**/*.ts*\"",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx}\"",
    "postinstall": "husky install .husky",
    "prepublishOnly": "clean-package"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "5.0.0-alpha.118",
    "@mui/material": "^5.11.7",
    "@mui/system": "^5.11.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.5",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-essentials": "7.0.0-beta.35",
    "@storybook/addon-interactions": "^7.0.0-beta.35",
    "@storybook/addon-links": "7.0.0-beta.35",
    "@storybook/addon-storyshots": "7.0.0-beta.35",
    "@storybook/node-logger": "7.0.0-beta.35",
    "@storybook/react": "7.0.0-beta.35",
    "@storybook/react-webpack5": "7.0.0-beta.35",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.19",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "clean-package": "^2.2.0",
    "commit-message-validator": "^1.0.2",
    "eslint": "^8.33.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-testing-library": "^5.10.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^13.1.0",
    "react-test-renderer": "^18.2.0",
    "rollup": "^3.14.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-node-externals": "^5.1.2",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^14.2.0",
    "storybook": "7.0.0-beta.35",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "4.9.5"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "@mui/material": {
      "optional": false
    },
    "@mui/base": {
      "optional": false
    },
    "@mui/lab": {
      "optional": false
    },
    "@mui/styled-engine": {
      "optional": false
    },
    "@mui/x-date-pickers": {
      "optional": false
    },
    "@mui/utils": {
      "optional": false
    },
    "@mui/icons-material": {
      "optional": false
    },
    "@emotion/cache": {
      "optional": false
    },
    "@emotion/react": {
      "optional": false
    },
    "@emotion/server": {
      "optional": false
    },
    "@emotion/styled": {
      "optional": false
    }
  },
  "peerDependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/base": "^5.0.0-alpha.108",
    "@mui/icons-material": "^5.10.16",
    "@mui/lab": "^5.0.0-alpha.110",
    "@mui/material": "^5.11.5",
    "@mui/styled-engine": "^5.10.16",
    "@mui/system": "^5.10.16",
    "@mui/utils": "^5.10.16",
    "@mui/x-date-pickers": "^5.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "clean-package": {
    "remove": [
      "scripts",
      "devDependencies",
      "publishConfig",
      "clean-package",
      "lint-staged",
      "husky",
      "config"
    ]
  },
  "lint-staged": {
    "src/**/*.+(js|jsx|ts|tsx)": [
      "npm run lint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commit-message-validator": {
      "pattern": "(build|ci|chore|docs|feat|fix|perf|refactor|revert|style|test)(\\((\\*|.{1,20})\\))?:",
      "errorMessage": "Invalid commit message"
    }
  }
}
